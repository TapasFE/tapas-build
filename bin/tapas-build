#!/usr/bin/env node
require('babel-core-resolve-enhance')({
  dirname: __dirname
});
require('babel-core/register');

var program = require('commander');

program
  .version(require('../package').version)
  .usage('[options] <entry> <output>')
  .option('-p, --production', 'Generate files which are under production environment.')
  .option('--index <index.html>', 'Specifies a index.html to inject css and js.')
  .option('--publicPath <http://example.com/foo/>', 'Specifies your the path to cdn.')
  .parse(process.argv);

require('../src/entry')(program);
